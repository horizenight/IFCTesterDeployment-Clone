{
  "version": 2,
  "builds": [
    {
      "src": "Backend/APP/main.py",
      "use": "@vercel/python",
      "config": { "projectDir": "Backend" }
    },
    {
      "src": "Frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Backend/APP/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend/$1"
    }
  ]
}
