{
  "version": 2,
  "builds": [
    {
      "src": "Backend/APP/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "includeFiles": ["Backend/APP/**/*.py"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Backend/APP/main.py"
    }
  ]
}
