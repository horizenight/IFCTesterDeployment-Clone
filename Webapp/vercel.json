{
  "version": 2,
  "builds": [
    {
      "src": "Backend/APP/main.py",
      "use": "@vercel/python",
      "config": { "projectDir": "Backend" }
    }
  ],
 "routes": [
    {
      "src": "/api/ids/(.*)",
      "dest": "Backend/App/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "Backend/App/main.py"
    }
  ]
}